# Questrade API Configuration
# Get your refresh token from: https://www.questrade.com/api/documentation/getting-started
QUESTRADE_REFRESH_TOKEN=your_questrade_refresh_token_here

# Lunch Money API Configuration
# Get your API token from: https://my.lunchmoney.app/developers
LUNCHMONEY_API_TOKEN=your_lunchmoney_api_token_here

# Questrade Account IDs to sync (comma-separated)
# You can get account IDs by calling the Questrade API /v1/accounts endpoint
QUESTRADE_ACCOUNT_IDS=12345678,87654321

# Number of days to sync (max 31 due to Questrade API limits)
SYNC_DAYS_BACK=31

# Optional: Lunch Money Asset ID to associate transactions with
# Leave blank if you don't want to associate with a specific asset
LUNCHMONEY_ASSET_ID=
